{
  "name": "spatialshot",
  "version": "1.0.0",
  "description": "Main application for rendering AI and contextual analysis module.",
  "homepage": "https://github.com/a7mddra/spatialshot",
  "author": "a7mddra <a7mddra@gmail.com>",
  "license": "Apache-2.0",
  "main": "source/main.js",
  "type": "commonjs",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder",
    "build:win": "electron-builder --win",
    "build:linux": "electron-builder --linux",
    "build:mac": "electron-builder --mac",
    "build:all": "electron-builder --win --mac --linux"
  },
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1"
  },
  "build": {
    "appId": "com.a7mddra.spatialshot",
    "productName": "SpatialShot",
    "asar": false,
    "icon": "assets/icons/light/512.png",
    "win": {
      "target": "dir",
      "icon": "assets/icons/light/512.png"
    },
    "linux": {
      "target": "dir",
      "icon": "assets/icons/light/512.png"
    },
    "mac": {
      "target": "dir",
      "icon": "assets/icons/light/512.png"
    }
  },
  "dependencies": {
    "axios": "^1.13.2",
    "electron-clipboard-watcher": "^1.0.1",
    "google-auth-library": "^10.5.0",
    "googleapis": "^166.0.0",
    "open": "^11.0.0"
  }
}
