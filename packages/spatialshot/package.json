{
  "name": "spatialshot",
  "version": "1.0.0",
  "description": "AI and contextual analysis module for screenshot data.",
  "main": "source/main.js",
  "type": "commonjs",
  "author": "a7mddra <a7mddra@gmail.com>",
  "license": "GPL-3.0",
  "scripts": {
    "start": "env -u IBUS_ADDRESS -u IBUS_DAEMON_PID -u IBUS_ENABLE_SYNC_MODE -u IBUS_SESSION_ID -u IBUS_USE_ASYNC_ENGINE -u IBUS_USE_PORTAL -u IBUS_VERSION electron .",
    "build:css": "sass source/login/style.scss:source/login/style.css",
    "build": "electron-builder"
  },
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1"
  },
  "build": {
    "appId": "com.a7mddra.spatialshot",
    "productName": "SpatialShot",
    "asar": false,
    "icon": "assets/icons/light/512.png",
    "win": {
      "target": "dir",
      "icon": "assets/icons/light/512.png"
    },
    "linux": {
      "target": "dir",
      "icon": "assets/icons/light/512.png"
    },
    "mac": {
      "target": "dir",
      "icon": "assets/icons/light/512.png"
    }
  },
  "dependencies": {
    "axios": "^1.13.2",
    "express": "^5.1.0",
    "google-auth-library": "^10.5.0",
    "googleapis": "^166.0.0",
    "localtunnel": "^2.0.2",
    "open": "^11.0.0"
  }
}
